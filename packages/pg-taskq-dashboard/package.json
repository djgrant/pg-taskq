    "gqless": "^0.0.1-alpha.29",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.1",
    "tailwindcss": "^1.5.1"
  },
  "devDependencies": {
    "@gqless/cli": "^0.0.1-alpha.31",
    "@types/jest": "^26.0.7",
    "@types/node": "^14.0.26",
    "@types/reach__router": "^1.3.5",
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "typescript": "^3.9.7"
